{
  "name": "strava-api-app",
  "version": "1.0.0",
  "type": "module",
  "description": "Strava visualization application",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev:frontend": "vite",
    "dev:backend": "node server.js",
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "build": "vite build && cp -r dist .",
    "postinstall": "npm run build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@react-three/drei": "^9.121.4",
    "@react-three/fiber": "^8.17.14",
    "@types/three": "^0.173.0",
    "axios": "1.6.7",
    "chart.js": "^4.4.1",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dayjs": "1.11.10",
    "dotenv": "16.4.1",
    "express": "4.18.2",
    "framer-motion": "11.0.3",
    "mongoose": "^8.9.7",
    "react": "18.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.22.0",
    "three": "^0.173.0"
  },
  "devDependencies": {
    "@types/react": "18.2.55",
    "@types/react-dom": "18.2.19",
    "@vitejs/plugin-react": "4.2.1",
    "autoprefixer": "10.4.17",
    "postcss": "8.4.35",
    "tailwindcss": "3.4.1",
    "typescript": "5.3.3",
    "vite": "5.0.12"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
